# gitattributes
*.h text eol=lf
*.hxx text eol=lf
*.hpp text eol=lf
*.in text eol=lf
*.c text eol=lf
*.cxx text eol=lf
*.cpp text eol=lf
*.txt text eol=lf
*.wav -diff binary sound
*.gif -diff binary image
*.jpg -diff binary image
*.jpeg -diff binary image
*.pbm -diff binary image
*.min.js -diff binary minified
*-min.js -diff binary minified
*.min.css -diff binary minified
/debian* debian-ignore
